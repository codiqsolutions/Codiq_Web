{
    "buildCommand": "cd frontend && npm install --production=false && npm run build",
    "outputDirectory": "frontend/.next",
    "installCommand": "cd frontend && npm install",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://codiq-backend.onrender.com/api/:path*"
        }
    ]
}